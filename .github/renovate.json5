{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "fmunteanu"
  ],
  "enabledManagers": [
    "github-actions",
    "gomod"
  ],
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependency"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "gomod"
      ],
      "rangeStrategy": "bump"
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
